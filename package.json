{
  "devDependencies": {
    "@ethereum-waffle/chai": "^3.0.1",
    "@nomiclabs/buidler": "^1.3.8",
    "@types/chai": "^4.2.11",
    "@types/mocha": "^7.0.2",
    "@types/node": "^13.11.0",
    "buidler-deploy": "^0.5.10",
    "buidler-ethers-v5": "^0.2.2",
    "chai": "^4.2.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "ethers": "^5.0.7",
    "solidity-coverage": "0.7.4",
    "ts-node": "^8.8.2",
    "typescript": "^3.8.3"
  },
  "scripts": {
    "postinstall": "node ./.setup.js",
    "test": "buidler test",
